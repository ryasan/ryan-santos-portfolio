{
  "name": "ryan-santos-portfolio",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:css && remix vite:build",
    "build:css": "sass app/styles/:public/styles",
    "dev": "concurrently \"npm run sass:dev\" \"remix vite:dev\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "sass:dev": "sass --watch app/styles/:public/styles",
    "serve": "netlify serve",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@netlify/edge-functions": "^2.3.1",
    "@netlify/remix-edge-adapter": "^3.2.1",
    "@netlify/remix-runtime": "^2.2.0",
    "@remix-run/node": "^2.12.0",
    "@remix-run/react": "^2.12.0",
    "@remix-run/serve": "^2.12.0",
    "clsx": "^2.1.1",
    "isbot": "^4.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "sass": "1.77.6",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
